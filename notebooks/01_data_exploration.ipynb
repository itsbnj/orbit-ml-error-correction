    "import requests\n",
    "\n",
    "url = \"https://celestrak.org/NORAD/elements/stations.txt\"\n",
    "response = requests.get(url)\n",
    "\n",
    "with open(\"../data/raw/iss_tle.txt\", \"w\") as f:\n",
    "    f.write(response.text)\n",
    "\n",
    "print(\"ISS TLE data downloaded and saved\")\n"
